        {
            "name": "CLI Patch",
            "type": "node",
            "request": "launch",
            "protocol": "inspector",
            "program": "${workspaceRoot}/cli/bin/script/cli.js",
            "stopOnEntry": false,
            "sourceMaps": true,
            "preLaunchTask": "gulp: build",
            "args": [
                "patch", "appName", "deploymentName", "-t", "1.0.0"
            ]
        },
        {
